<Window x:Class="Demo.DataBinding.step_01.MyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Demo.DataBinding.step_01"
        mc:Ignorable="d"
        Title="MyWindow" Height="450" Width="800">
    
    <Window.DataContext>
        <local:ModelClass/>
    </Window.DataContext>
    
    <Grid>
        <TextBox 
            x:Name="VariableThatUsedInCsPartialFile"
            Text="{Binding ModelStringProperty,UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">
        </TextBox>
    </Grid>
</Window>
